{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "appConfiguration": {
      "enabled": false
    }
  },
  "logging": {
    "logLevel": {
      "Default": "Warning",
      "Host.Results": "Error",
      "Host.Startup": "Warning",
      "Function": "Information"
    }
  }
}